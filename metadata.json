{
  "name": "simp-simp_grafana",
  "version": "1.1.0",
  "author": "SIMP Team",
  "summary": "A profile module to integrate Grafana with SIMP",
  "license": "Apache-2.0",
  "source": "https://github.com/simp/pupmod-simp-simp_grafana",
  "project_page": "https://github.com/simp/pupmod-simp-simp_grafana",
  "issues_url": "https://simp-project.atlassian.net",
  "tags": [
    "simp"
  ],

  "data_provider": "hiera",

  "dependencies": [
    {
      "name": "puppet/grafana",
      "version_requirement": ">= 3.0.0 < 5.0.0"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.13.1 < 6.0.0"
    },
    {
      "name": "simp/iptables",
      "version_requirement": ">= 6.0.0 < 7.0.0"
    },
    {
      "name": "simp/pki",
      "version_requirement": ">= 6.0.0 < 7.0.0"
    },
    {
      "name": "simp/simp_openldap",
      "version_requirement": ">= 6.0.0 < 7.0.0"
    },
    {
      "name": "simp/simplib",
      "version_requirement": ">= 3.1.0 < 4.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "OracleLinux",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 4.7.0 < 6.0.0"
    }
  ]
}
